                            android:padding="@dimen/_8sdp"
                            android:weightSum="6">


                        </LinearLayout>
            -->

            <View
                android:id="@+id/line1"
                android:layout_width="wrap_content"
                android:layout_height="1dp"
                android:layout_below="@+id/linear_2"
                android:layout_marginLeft="@dimen/_8sdp"

                android:layout_marginRight="@dimen/_8sdp"
                android:background="@color/gray"></View>

            <TableLayout
                android:id="@+id/tbTableLayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_below="@+id/line1"
                android:padding="@dimen/_8sdp">

                <TableRow>

                    <LinearLayout
                        android:id="@+id/linearfirst"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/line1"
                        android:layout_margin="@dimen/_8sdp"
                        android:layout_marginLeft="@dimen/_8sdp"
                        android:gravity="left">

                        <com.yberry.dinehawaii.customview.CustomTextView
                            android:id="@+id/customTextView18"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Original Amount Due : $"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                            app:edittextfont="Raleway-Regular.ttf" />

                        <com.yberry.dinehawaii.customview.CustomEditText
                            android:id="@+id/originalAmountEt"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                            app:edittextfont="Raleway-Regular.ttf" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:background="@drawable/help"/>

                    </LinearLayout>

                </TableRow>

                <TableRow>

                    <LinearLayout
                        android:id="@+id/linearsecound"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/linearfirst"
                        android:layout_margin="@dimen/_8sdp"
                        android:layout_marginLeft="@dimen/_8sdp">

                        <com.yberry.dinehawaii.customview.CustomTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Adjust Amount Due : $"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                            app:edittextfont="Raleway-Regular.ttf" />

                        <com.yberry.dinehawaii.customview.CustomEditText
                            android:id="@+id/adjustAmountET"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:layout_marginLeft="@dimen/_10sdp"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                            app:edittextfont="Raleway-Regular.ttf" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:background="@drawable/help"/>

                    </LinearLayout>

                </TableRow>

                <TableRow>

                    <LinearLayout
                        android:id="@+id/linearthird"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/_8sdp"
                        android:layout_marginLeft="@dimen/_8sdp">

                        <com.yberry.dinehawaii.customview.CustomTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="GE Tax : $"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                            app:edittextfont="Raleway-Regular.ttf" />

                        <com.yberry.dinehawaii.customview.CustomEditText
                            android:id="@+id/taxET"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                            android:layout_marginLeft="@dimen/_70sdp"
                            app:edittextfont="Raleway-Regular.ttf" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:background="@drawable/help"/>

                    </LinearLayout>

                </TableRow>

                <TableRow>

                    <LinearLayout
                        android:id="@+id/linearfour"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/_8sdp"
                        android:layout_marginLeft="@dimen/_8sdp" >

                        <com.yberry.dinehawaii.customview.CustomTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Delivery Fee : $ "
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                            app:edittextfont="Raleway-Regular.ttf" />

                        <com.yberry.dinehawaii.customview.CustomEditText
                            android:id="@+id/deliveryFeeET"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                            android:layout_marginLeft="@dimen/_45sdp"
                            app:edittextfont="Raleway-Regular.ttf" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:background="@drawable/help"/>

                    </LinearLayout>

                </TableRow>

                <TableRow>

                    <LinearLayout
                        android:id="@+id/linearfive"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/linearfour"
                        android:layout_margin="@dimen/_8sdp"
                        android:layout_marginLeft="@dimen/_8sdp">

                        <com.yberry.dinehawaii.customview.CustomTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Service Fee(if applicable) : $"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                            app:edittextfont="Raleway-Regular.ttf" />

                        <com.yberry.dinehawaii.customview.CustomEditText

                            android:id="@+id/serviceFeeET"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small" />

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:background="@drawable/help"/>

                    </LinearLayout>

                </TableRow>

                <TableRow>

                    <LinearLayout
                        android:id="@+id/linearsix"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="@dimen/_8sdp"
                        android:layout_marginLeft="@dimen/_8sdp">

                        <com.yberry.dinehawaii.customview.CustomTextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Total Due : $ "

                            android:textAppearance="@style/TextAppearance.AppCompat.Small"
                            app:edittextfont="Raleway-Regular.ttf" />

                        <com.yberry.dinehawaii.customview.CustomEditText
                            android:id="@+id/totalDueEt"
                            android:layout_width="@dimen/_50sdp"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:layout_marginLeft="@dimen/_60sdp"
                            android:textAppearance="@style/TextAppearance.AppCompat.Small"/>

                        <ImageView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:background="@drawable/help"/>

                    </LinearLayout>

                </TableRow>
            </TableLayout>

            <TextView
                android:id="@+id/confirm"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tbTableLayout"
                android:layout_centerInParent="true"
                android:background="@drawable/button"
                android:padding="@dimen/_8sdp"
                android:text="DELIVERY CONFIRMATION DATE"
                android:textColor="@color/green"
                android:textAppearance="@style/TextAppearance.AppCompat.Subhead" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/confirm"
                android:orientation="vertical"
                android:paddingTop="@dimen/_8sdp">

                <Button
                    android:id="@+id/amount"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Original Amount Date:"
                    android:textAppearance="@style/TextAppearance.AppCompat.Small" />

                <Button
                    android:id="@+id/adjust"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Adjust Amount Date:"
                    android:textAppearance="@style/TextAppearance.AppCompat.Small" />

                <RelativeLayout
                    android:layout_width="@dimen/_50sdp"
                    android:layout_height="@dimen/_50sdp"
                    android:layout_gravity="right"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentRight="true"
                    android:layout_below="@+id/llBussiness"
                    android:padding="@dimen/_10sdp"
                    android:gravity="center" >

                <ImageView
                    android:id="@+id/imageview"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/cont" />
</RelativeLayout>

                <!--  <ImageView
                      android:id="@+id/imageview"
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:layout_gravity="right"
                      android:layout_marginBottom="@dimen/_8sdp"
                      android:layout_marginRight="@dimen/_18sdp"
                      android:layout_marginTop="@dimen/_78sdp"
                      android:src="@drawable/cont" />-->
            </LinearLayout>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>